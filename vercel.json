{
  "version": 2,
  "buildCommand": "cd server && npm install && npx prisma generate",
  "installCommand": "cd server && npm install",
  "builds": [
    {
      "src": "server/src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server/src/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
